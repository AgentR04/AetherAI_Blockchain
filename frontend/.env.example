# Aptos Network Configuration
REACT_APP_APTOS_NODE_URL=https://fullnode.devnet.aptoslabs.com/v1
REACT_APP_APTOS_NETWORK=devnet

# Smart Contract Configuration
# Replace with your deployed smart contract address
REACT_APP_DEFI_AGENT_ADDRESS=0x...
REACT_APP_MODULE_NAME=defi_agent

# API Keys and External Services
# Get your API key from https://www.coingecko.com/api/pricing
REACT_APP_COINGECKO_API_KEY=your_coingecko_api_key_here
# Get your API key from your market data provider
REACT_APP_MARKET_DATA_API_KEY=your_market_data_api_key_here

# Application Configuration
REACT_APP_DEFAULT_CREDIT_SCORE=50
REACT_APP_MIN_TRANSACTION_AMOUNT=1
REACT_APP_MAX_TRANSACTION_AMOUNT=1000000

# Feature Flags
REACT_APP_ENABLE_BIOMETRIC_AUTH=true
REACT_APP_ENABLE_AI_FEATURES=true
REACT_APP_ENABLE_MARKET_ANALYSIS=true
